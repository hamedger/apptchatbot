{
  "name": "arlington-whatsapp-bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "frontend": "cd src && npm start",
    "build": "cd src && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "security": "npm audit",
    "security:fix": "npm audit fix",
    "postinstall": "cd src && npm install",
    "render-build": "npm ci --only=production && cd src && npm ci && npm run build && cd .."
  },
  "keywords": ["whatsapp", "bot", "appointments", "carpet-cleaning"],
  "author": "",
  "license": "ISC",
  "description": "WhatsApp appointment chatbot for Arlington Steamers Carpet Cleaning",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "twilio": "^5.8.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "express-slow-down": "^2.0.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0"
  }
}